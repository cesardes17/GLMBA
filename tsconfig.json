{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@src/*": ["./src/*"],
      "@components/*": ["./components/*"],
      "@screens/*": ["./app/*"],
      "@assets/*": ["./assets/*"],
      "@utils/*": ["./utils/*"],
      "@hooks/*": ["./hooks/*"],
      "@types/*": ["./types/*"],
      "@services/*": ["./services/*"],
      "@constants/*": ["./constants/*"],
      "@coreServices/*": ["./src/service/core/*"]
    }
  }
}
